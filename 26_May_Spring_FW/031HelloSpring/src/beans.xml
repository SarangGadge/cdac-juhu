<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
     http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
     http://www.springframework.org/schema/context
     http://www.springframework.org/schema/context/spring-context-4.0.xsd">
     <!-- configuring the WatsAppMsgService bean 
     so that spring container will create the bean 
     with unique name as id "watsAppMessage" -->
     <bean class="com.cdac.spring.WatsAppMsgService" 
     id="watsAppMessage"></bean>
     
     <bean class="com.cdac.spring.cab.service.OlaCabService" 
     id="olaCabService">
     </bean>
     <bean class="com.cdac.spring.cab.service.UberCabService" 
     id="uberCabService">
     </bean>
</beans>